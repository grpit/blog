{
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "jsx-quotes":"off",
    "react/jsx-props-no-spreading": "off",
    "@typescript-eslint/comma-dangle": "off"
  }
}
